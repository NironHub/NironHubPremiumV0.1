-- Script Execution Utilities
local ExecutionLogger = {}
ExecutionLogger.History = {}

function ExecutionLogger:Log(scriptName, scriptContent, success)
    if CONFIG.LOG_EXECUTION then
        table.insert(self.History, {
            name = scriptName,
            time = os.time(),
            success = success
        })
        
        -- Only keep the last 100 entries
        if #self.History > 100 then
            table.remove(self.History, 1)
        end
    end
end

function ExecutionLogger:GetHistory()
    return self.History
end

-- Security check function
local function PerformSecurityCheck(scriptContent)
    -- Simple security checker
    local potentiallyHarmful = false
    local dangerousStrings = {
        "game:Shutdown",
        "game:GetService('Players').LocalPlayer:Kick",
        "for i = 1, 1000000 do",
        "while true do",
        "DeleteFile",
        "HttpPost"
    }
    
    for _, checkString in ipairs(dangerousStrings) do
        if string.find(scriptContent, checkString) then
            potentiallyHarmful = true
            break
        end
    end
    
    return potentiallyHarmful
end

-- Safe execution wrapper
local function SafeExecute(scriptName, scriptCode)
    -- Update loading text
    StatusText.Text = "Executing: " .. scriptName
    
    -- Check if code looks potentially harmful
    local isHarmful = PerformSecurityCheck(scriptCode)
    
    if isHarmful then
        Rayfield:Notify({
            Title = "⚠️ Security Warning",
            Content = "This script contains potentially harmful code. Execute anyway?",
            Duration = 10,
            Image = 4914902889,
            Actions = {
                Confirm = {
                    Name = "Execute Anyway",
                    Callback = function()
                        -- Execute with error handling
                        local success, err = pcall(function()
                            loadstring(scriptCode)()
                        end)
                        
                        if success then
                            Rayfield:Notify({
                                Title = "Executed Successfully",
                                Content = "Running: " .. scriptName,
                                Duration = 3,
                                Image = 4483362458
                            })
                            ExecutionLogger:Log(scriptName, scriptCode, true)
                        else
                            Rayfield:Notify({
                                Title = "Execution Failed",
                                Content = "Error: " .. tostring(err):sub(1, 100),
                                Duration = 5,
                                Image = 4914902889
                            })
                            ExecutionLogger:Log(scriptName, scriptCode, false)
                        end
                    end
                },
                Cancel = {
                    Name = "Cancel",
                    Callback = function()
                        Rayfield:Notify({
                            Title = "Execution Cancelled",
                            Content = "Script was not executed for safety reasons",
                            Duration = 3,
                            Image = 4914902889
                        })
                    end
                }
            }
        })
    else
        -- Execute with error handling
        local success, err = pcall(function()
            loadstring(scriptCode)()
        end)
        
        if success then
            Rayfield:Notify({
                Title = "Executed Successfully",
                Content = "Running: " .. scriptName,
                Duration = 3,
                Image = 4483362458
            })
            ExecutionLogger:Log(scriptName, scriptCode, true)
        else
            Rayfield:Notify({
                Title = "Execution Failed",
                Content = "Error: " .. tostring(err):sub(1, 100),
                Duration = 5,
                Image = 4914902889
            })
            ExecutionLogger:Log(scriptName, scriptCode, false)
        end
    end
end

