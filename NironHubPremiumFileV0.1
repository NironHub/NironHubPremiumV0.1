GunsTab:CreateToggle({
    Name = "No Recoil",
    Info = "Removes recoil from guns for better accuracy",
    CurrentValue = false,
    Flag = "NoRecoilToggle",
    Callback = function(Value)
        -- Setup no recoil script
        _G.noRecoil = Value
        
        if Value then
            -- Create hook for new guns
            _G.noRecoilConnection = LocalPlayer.Backpack.ChildAdded:Connect(function(tool)
                if tool:IsA("Tool") then
                    task.wait()
                    if tool:FindFirstChild("RecoilMod") then
                        tool.RecoilMod.Value = 0
                    end
                    
                    -- Look for recoil scripts
                    for _, obj in pairs(tool:GetDescendants()) do
                        if obj:IsA("Script") and (obj.Name:lower():find("recoil") or obj.Name:lower():find("spread")) then
                            obj.Disabled = true
                        end
                    end
                end
            end)
            
            -- Apply to existing guns
            for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
                if tool:IsA("Tool") then
                    if tool:FindFirstChild("RecoilMod") then
                        tool.RecoilMod.Value = 0
                    end
                    
                    -- Look for recoil scripts
                    for _, obj in pairs(tool:GetDescendants()) do
                        if obj:IsA("Script") and (obj.Name:lower():find("recoil") or obj.Name:lower():find("spread")) then
